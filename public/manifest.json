{
    "manifest_version": 3,
    "name": "Upwork Assistant",
    "version": "1.0",
    "description": "A Chrome extension to assist with Upwork job applications.",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "declarativeNetRequest",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "*://*.upwork.com/*"
    ]
}